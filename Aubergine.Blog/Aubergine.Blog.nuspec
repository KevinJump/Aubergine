<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2011/10/nuspec.xsd">
  <metadata>
      <id>Aubergine.Blog</id>
      <version>1.0.0-Beta1</version>
      <title>Aubergine for Umbraco : Blogging</title>
      <authors>Kevin Jump</authors>
      <owners>Kevin Jump</owners>
      <requireLicenseAcceptance>false</requireLicenseAcceptance>
      <description>Basic Member Authentication</description>
      <releaseNotes>Initial Release</releaseNotes>
      <dependencies>
        <dependency id="Aubergine.Comments" version="1.0.0-Beta1" />
