<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2011/10/nuspec.xsd">
  <metadata>
      <id>Aubergine.Forums</id>
      <version>1.0.0</version>
      <title>Aubergine for Umbraco : Blogging</title>
      <authors>Kevin Jump</authors>
      <owners>Kevin Jump</owners>
      <requireLicenseAcceptance>false</requireLicenseAcceptance>
      <description>Basic Member Authentication</description>
      <releaseNotes>Initial Release</releaseNotes>
      <dependencies>
        <dependency id="Aubergine.UserContent" version="1.0.0" />
      </dependencies>
  </metadata>
  <files>
    <file src=".\bin\Release\Aubergine.Forums.dll" target="lib\net45\" />
    <file src=".\Views\**\*.*" target="content\Views\" />
    <file src=".\css\**\*.*" target="content\css\" />
    <file src="..\install.ps1" target="tools"/>
    <file src=".\_data\**\*.*" target="data\forums"/>
  </files>
</package>